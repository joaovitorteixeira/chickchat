{
  "editor.formatOnSave": true,
  "cSpell.words": [
    "Chickchat",
    "chrono",
    "codegen",
    "dotenv",
    "icondata",
    "Insertable",
    "leptos",
    "reqwest",
    "serde",
    "stylance"
  ],
  "rust-analyzer.rustfmt.overrideCommand": [
    "leptosfmt",
    "--stdin",
    "--rustfmt"
  ],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "files.associations": {
    "*.rs": "rust"
  },
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "css.validate": false
}
